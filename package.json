{
	"name": "masked-garden",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"dev:sound": "vite --config vite.config.sound.ts",
		"dev:art": "vite --config vite.config.art.ts",
		"dev:particles": "vite --config vite.config.particles.ts",
		"dev:assets": "vite --config vite.config.assets.ts",
		"build": "vite build",
		"preview": "vite preview",
		"check": "pnpm typecheck && pnpm lint",
		"typecheck": "tsc --noEmit",
		"lint": "oxlint .",
		"fmt": "oxlint --fix . && oxfmt env ent"
	},
	"dependencies": {
		"jotai": "^2.12.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"three": "^0.172.0"
	},
	"devDependencies": {
		"@types/node": "^22.0.0",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"@types/three": "^0.172.0",
		"@vitejs/plugin-react": "^4.4.0",
		"oxfmt": "^0.27.0",
		"oxlint": "^1.42.0",
		"typescript": "^5.7.0",
		"vite": "^6.1.0"
	},
	"engines": {
		"node": ">=20",
		"pnpm": ">=9"
	},
	"packageManager": "pnpm@9.15.0"
}
